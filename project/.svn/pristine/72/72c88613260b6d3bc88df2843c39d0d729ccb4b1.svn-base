#dao ul .shou .box div:nth-child(1) {
  border-bottom: 1px dashed #F1F1F1; }

#dao ul .shou .box div:nth-child(2) {
  border-bottom: 1px dashed #F1F1F1; }

#dao ul .shou .box div:nth-child(3) {
  border-bottom: 1px dashed #F1F1F1; }

#dao ul .shou .box div:nth-child(4) {
  border-bottom: 1px dashed #F1F1F1; }

#dao ul .shou:hover .box {
