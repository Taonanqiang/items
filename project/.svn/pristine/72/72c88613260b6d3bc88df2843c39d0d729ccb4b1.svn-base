  display: block;
  padding-left: 5px; }

nav .menu .gz a {
  padding-right: 5px; }

nav .menu .hnews a {
  display: block;
  font: 12px/42px "";
  color: #333;
  width: 60px;
  padding: 0; }

nav .menu span {
  margin-right: 7px; }

nav .menu a span:nth-child(1) {
